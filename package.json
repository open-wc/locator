  },
  "devDependencies": {
    "@types/chrome": "0.0.123",
    "chokidar-cli": "^2.1.0",
    "mversion": "^2.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-index-html": "^1.5.9",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
